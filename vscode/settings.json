{
    "files.autoGuessEncoding": true,
    "workbench.iconTheme": "vs-nomo-dark",
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontLigatures": null,
    "codic.ACCESS_TOKEN": "U2oE9YIyPKSedN05UiD5l2WxptrEVmbehq",
    "codic.case": "camelCase",
    "editor.minimap.enabled": false,
    "terminal.integrated.fontFamily": "monospace",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Atom One Dark",
    "powermode.enabled": true,
    "powermode.combo.location": "statusbar",
    "powermode.combo.counterEnabled": "hide",
    "powermode.combo.counterSize": 0,
    "powermode.shake.enabled": false,
    "powermode.explosions.offset": 0,
    "powermode.explosions.duration": 500,
    "vscode-pets.position": "explorer",
    "vscode-pets.petSize": "medium",
    "vscode-pets.petType": "dog",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true, 
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "python.linting.lintOnSave": true,
    "python.linting.flake8Args": [
        "--ignore=W293, W504",
        "--max-line-length=150",
        "--max-complexity=20",
    ],
    "python.formatting.provider": "autopep8",
    "python.formatting.autopep8Args": [
        "--aggressive", "--aggressive",
    ],
    "cSpell.userWords": [
        "cardsection",
        "CHROMEDRIVER",
        "connectorcard",
        "dotenv",
        "etext",
        "Haruto",
        "kintai",
        "Kyuka",
        "msteams",
        "openpyxl",
        "printl",
        "pymsteams",
        "Regist",
        "REUSEADDR",
        "SIGTERM",
        "workflowdiv",
        "xlwings",
        "Yukyu"
    ],
    "pasteImage.path": "${currentFileDir}/./imgs",
    "[python]": {
        "editor.formatOnType": true
    },
}